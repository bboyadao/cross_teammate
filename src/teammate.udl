namespace teammate {

};

dictionary Act {
    string user;
    i64 paid;
    i64 have_to_pay;
    i64 need_to_earn;
};

dictionary Payment {
    string src;
    string dst;
    i64 amount;
};

[Error]
enum ArithmeticError {
  "IntegerOverflow",
};


interface Teammate {
    Act new_act(string user, i64 paid);
    sequence<Payment> calculate(sequence<Act> acts, optional boolean ceil);
};
